#include <bits/stdc++.h>

using namespace std;

int main(){
    int r;
    int c;
    cin >> r >> c;
    int n,x;
    if(r>=1 && c<=20)
    for(int i=0;i<r;i++)
        {
            n=2;
            while(n>=0)
            {
                x=c;
                if(n%2==0)
                    while(x>0)
                    {
                        cout<<"..O..";
                         x--;
                    }
                else
                    while(x>0)
                    {
                        cout<<"O.o.O";
                         x--;
                    }
                cout<<"\n";
                n--;
            }
        
        }
    return 0;
}
